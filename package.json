{
  "name": "react-ts-audio-recorder",
  "version": "1.1.3",
  "description": "Framework-agnostic MP3 voice message recorder inspired by Kagami/vmsg.",
  "keywords": [
    "react",
    "audio",
    "recorder",
    "hooks",
    "web-audio",
    "mp3",
    "wav",
    "webm"
  ],
  "author": "ThangDevAlone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thangdevalone/react-audio-recorder.git"
  },
  "bugs": {
    "url": "https://github.com/thangdevalone/react-audio-recorder/issues"
  },
  "homepage": "https://github.com/thangdevalone/react-audio-recorder#readme",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./assets/vmsg.wasm": "./assets/vmsg.wasm",
    "./assets/pcm-worklet.js": "./assets/pcm-worklet.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  }
}